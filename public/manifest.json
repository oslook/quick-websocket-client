{
  "manifest_version": 3,
  "name": "WebSocket Client",
  "version": "1.0",
  "description": "A WebSocket client for testing WebSocket connections",
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage"],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
} 